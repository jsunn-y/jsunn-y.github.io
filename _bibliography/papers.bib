	author = {Yang, Jason and Mora, Ariane and Liu, Shengchao and Wittmann, Bruce J. and Anandkumar, Anima and Arnold, Frances H. and Yue, Yisong},
	month = jun,
	year = {2024},
	html = {http://arxiv.org/abs/2406.15669},
	file = {Yang et al. - 2024 - CARE a Benchmark Suite for the Classification and.pdf:/Users/jason/Zotero/storage/JHSCCKX4/Yang et al. - 2024 - CARE a Benchmark Suite for the Classification and.pdf:application/pdf},
	preview={creep.png},
	selected={true},
	code={https://github.com/jsunn-y/CARE},
